{
	"name": "jshint",
	"description": "JSHint your JavaScript",
	"version": "0.0.1",
	"dependencies" : {
		"jshint" : "0.7.x"
	},
	"systemDependencies": {
		"node": {
			"version": ">= 0.8.7",
			"install": "brew update && brew install node",
			"upgrade": "brew update && brew upgrade node"
		},
		"npm": {
			"version": ">= 1.1.59",
			"install": "brew update && brew install node",
			"upgrade": "brew update && brew upgrade node"
		}
	},
	"config": {
		"replaceVars": false
	}
}
