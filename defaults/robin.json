{
	"name": "robin",
	"version": "3.0.0",
	"author": "RED Interactive <geeks@ff0000.com>",
	"description": "A highly customizable boilerplate to initialize common projects.",
	"repository": {
		"type": "git",
		"url": "__PROJECT_REPOSITORY__"
	},
	"vars": {},
	"dirs": {
		"robin": ".robin",
		"tasks": "robin/tasks",
		"config": "robin/config"
	},
	"ignoreTasks": [
		"concat",
		"init",
		"lint",
		"min",
		"qunit",
		"test"
	],
	"requiredPaths": [
		".robin",
		"node_modules"
	],
	"excludedPaths": [
		".git",
		".robin",
		"node_modules"
	],
	"installedPlugins": {},
	"systemDependencies": {
		"grunt": ">=0.3.0"
	},
	"engines": {
		"node": "*"
	}
}
